{
  "name": "minimal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --ssl-key /path/to.key --ssl-cert /path/to.crt -o",
    "build": "ng build",
    "build:all": "ng build --prod && ng build --prod --app 1",
    "build:aot": "ng build --prod --aot && ng build --prod --aot --app 1",
    "build:optimizer": "ng build --prod --aot --build-optimizer && ng build --prod --aot --build-optimizer --app 1",
    "pwa": "ng build --prod && sw-precache --root=dist  --config=precache-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.4.0",
    "@angular/core": "^4.4.0",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^4.4.0",
    "@angular/http": "^4.3.6",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "core-js": "^2.5.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "rxjs": "^5.4.3",
    "socket.io-client": "^2.0.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.4.5",
    "@angular/compiler-cli": "^4.4.0",
    "@angular/language-service": "^4.3.6",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "sw-precache-webpack-plugin": "^0.11.4",
    "typescript": "~2.3.3"
  }
}
